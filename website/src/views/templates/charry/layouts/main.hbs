<body class="header-sticky home-boxed">
	<div class="loader">
		<span class="loader1 block-loader"></span>
		<span class="loader2 block-loader"></span>
		<span class="loader3 block-loader"></span>
	</div>

	<!-- Header -->
	<header id="header" class="header clearfix">
		<div class="header-wrap clearfix">
			<div id="logo" class="logo">
				<a href="/" rel="home">
					<img src="images/logo.png" alt="logo" />
				</a>
			</div>
			<!-- /.logo -->
			<div class="nav-wrap">
				<div class="btn-menu">
					<span></span>
				</div>
				<!-- //mobile menu button -->
				<nav id="mainnav" class="mainnav">
					<ul class="menu">

						<li {{#ifEquals slug "home" }}class="home" {{/ifEquals}}><a href="/">Home</a></li>
						{{#each links}}

						<li {{#ifEquals ../page.slug linkValue }}class="home" {{/ifEquals}}>
							<a {{#if newTab}}target="_blank" {{/if}} {{#ifEquals type "page" }}href="/{{linkValue}}"
								{{else}}href="{{linkValue}}" {{/ifEquals}}>{{label}}</a>
						</li>

						{{/each}}

						{{#if site.livestream}}<li><a target="_blank" href="{{site.livestreamLink}}">Sermons</a>
						</li>{{/if}}
						{{#if site.giving}}<li><a target="_blank" href="{{site.givingLink}}">Give</a></li>
						{{/if}}

					</ul>
					<!-- /.menu -->
				</nav>
				<!-- /.mainnav -->
			</div>
			<!-- /.nav-wrap -->

			<div class="flat-information">
				<ul class="flat-socials">
					{{#if site.facebook}}
					<li class="facebook">
						<a target="_blank" href="{{site.facebook}}">
							<i class="fa fa-facebook"></i>
						</a>
					</li>
					{{/if}}
					{{#if site.twitter}}
					<li class="twitter">
						<a target="_blank" href="{{site.twitter}}">
							<i class="fa fa-twitter"></i>
						</a>
					</li>
					{{/if}}
					{{#if site.instagram}}
					<li class="instagram">
						<a target="_blank" href="{{site.instagram}}">
							<i class="fa fa-instagram"></i>
						</a>
					</li>
					{{/if}}
					{{#if site.youtube}}
					<li class="youtube">
						<a target="_blank" href="{{site.youtube}}">
							<i class="fa fa-youtube"></i>
						</a>
					</li>
					{{/if}}
				</ul>

			</div>
		</div>
		<!-- /.header-inner -->
	</header>
	<!-- /.header -->

	{{{body}}}

	<footer class="footer">
		<div class="footer-widgets">
			<div class="container">
				<div class="row">
					<div class="col-md-9">
						<div class="widget widget-text">
							<h4 class="widget-title">About Compassion</h4>
							<div class="text">
								<p>{{site.footerNote}}</p>
							</div>
							<!-- /.textwidget -->
							<ul class="flat-socials">
								{{#if site.facebook}}
								<li class="facebook">
									<a target="_blank" href="{{site.facebook}}">
										<i class="fa fa-facebook"></i>
									</a>
								</li>
								{{/if}}
								{{#if site.twitter}}
								<li class="twitter">
									<a target="_blank" href="{{site.twitter}}">
										<i class="fa fa-twitter"></i>
									</a>
								</li>
								{{/if}}
								{{#if site.instagram}}
								<li class="instagram">
									<a target="_blank" href="{{site.instagram}}">
										<i class="fa fa-instagram"></i>
									</a>
								</li>
								{{/if}}
								{{#if site.youtube}}
								<li class="youtube">
									<a target="_blank" href="{{site.youtube}}">
										<i class="fa fa-youtube"></i>
									</a>
								</li>
								{{/if}}
							</ul>
						</div>
						<!-- /.widget -->
					</div>
					<!-- /.col-md-5 -->

					<div class="col-md-3">
						<div class="widget contact-info">
							<h4 class="widget-title">Address</h4>
							<ul>
								{{#if site.address}}<li class="address"><a href="#">{{site.address}}</a></li>
								{{/if}}
								{{#if site.city}}<li class="address1"><a href="#">{{site.city}},
										{{site.state}} {{site.zip}}</a></li>{{/if}}
								{{#if site.phone}}<li class="phone"><a href="#">{{site.phone}}</a></li>{{/if}}
								{{#if site.email}}<li class="email"><a href="#">{{site.email}}</a></li>{{/if}}
							</ul>
						</div>
					</div>
					<!-- /.col-md-3 -->

					{{!-- <div class="col-md-4">
						<div class="widget widget_mc4wp">
							<div id="mc4wp-form-1" class="form mc4wp-form clearfix">
								<h4 class="widget-title">Subscribe Us</h4>
								<div class="mail-chimp">
									<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor
										incididunt ut labore et dolore magna amader liqua.</p>
									<form action="#" id="mailform" method="get">
										<input type="email" id="m" class="mmm" placeholder="Your Email" />
										<input type="submit" value="ïƒ " id="gmail" />
									</form>
								</div>
							</div>
						</div>
					</div> --}}
					<!-- /.col-md-4 -->
				</div>
				<!-- /.row -->
			</div>
			<!-- /.container -->
		</div>
		<!-- /.footer-widgets -->
	</footer>
	<!-- /footer -->

	<div class="bottom">
		<div class="container-bottom">
			<div class="copyright">
				<p>&copy; 2021 Compassion Church. All Rights Reserved</p>
			</div>

			<ul class="text-right">
				<li></li>
				{{!-- <li><a href="#">Terms &amp; Condition</a></li> --}}
				{{!-- <li><a href="#">Privacy Policy</a></li> --}}
			</ul>
		</div>
		<!-- /.container-bottom -->
	</div>
