	<script type="text/javascript" src="templates/charry/js/jquery.min.js"></script>
	<script type="text/javascript" src="templates/charry/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="templates/charry/js/jquery.easing.js"></script>
	<script type="text/javascript" src="templates/charry/js/owl.carousel.js"></script>
	<script type="text/javascript" src="templates/charry/js/jquery.flexslider.js"></script>
