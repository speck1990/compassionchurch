<div class="page-title parallax parallax4" style="background-image:url({{page.hero}})">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="page-title-heading">
					<h2 class="title">{{page.title}}</h2>
				</div>
				<!-- /.page-title-heading -->
			</div>
			<!-- /.col-md-12 -->
		</div>
		<!-- /.row -->
	</div>
	<!-- /.container -->
</div>
<!-- /page-title parallax -->

{{#each page.content}}

{{#switch type}}

{{#case 'section'}}

<section class="flat-row">
	<div class="container content-container">
		<div class="row">

			{{#each content}}
			{{> blocks}}
			{{/each}}

		</div>
	</div>
</section>

{{/case}}

{{/switch}}




{{/each}}